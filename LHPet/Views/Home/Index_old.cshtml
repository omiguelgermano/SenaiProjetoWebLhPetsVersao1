@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    
    @foreach (var item in ViewBag.listaClientes) {
        
        <div>@item.Id</div>
        <div>@item.Name</div>
        <div>@item.Cpf</div>
    }

    @foreach (var item in ViewBag.listaFornecedores) {

        <div>@item.Id</div>
        <div>@item.Name</div>
        <div>@item.Cnpj</div>
    }
</div>
